/sdtdatahash" xmlns:w16se="http://schemas.microsoft.com/office/word/2015/wordml/symex" xmlns:wpg="http://schemas.microsoft.com/office/word/2010/wordprocessingGroup" xmlns:wpi="http://schemas.microsoft.com/office/word/2010/wordprocessingInk" xmlns:wne="http://schemas.microsoft.com/office/word/2006/wordml" xmlns:wps="http://schemas.microsoft.com/office/word/2010/wordprocessingShape" mc:Ignorable="w14 w15 w16se w16cid w16 w16cex w16sdtdh wp14"><w:comment w:initials="ABdS" w:author="Aline Silva" w:date="2022-08-11T14:46:00Z" w:id="4"><w:p w:rsidR="002E4356" w:rsidP="009F2E37" w:rsidRDefault="002E4356" w14:paraId="09E76CBE" w14:textId="77777777"><w:pPr><w:pStyle w:val="Textodecomentrio" /></w:pPr><w:r><w:rPr><w:rStyle w:val="Refdecomentrio" /></w:rPr><w:annotationRef /></w:r><w:r><w:t>@thiago, tem como criar um ícone que ilustre as atividades?</w:t></w:r></w:p></w:comment><w:comment w:initials="ABdS" w:author="Aline Silva" w:date="2022-08-11T15:12:00Z" w:id="9"><w:p w:rsidR="009711BD" w:rsidP="00C51954" w:rsidRDefault="009711BD" w14:paraId="37FB6D62" w14:textId="77777777"><w:pPr><w:pStyle w:val="Textodecomentrio" /></w:pPr><w:r><w:rPr><w:rStyle w:val="Refdecomentrio" /></w:rPr><w:annotationRef /></w:r><w:r><w:t>Confirmar.</w:t></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r></w:p></w:comment><w:comment w:initials="ABdS" w:author="Aline Silva" w:date="2022-08-11T15:13:00Z" w:id="10"><w:p w:rsidR="008D22FF" w:rsidP="007C683B" w:rsidRDefault="008D22FF" w14:paraId="6D203490" w14:textId="77777777"><w:pPr><w:pStyle w:val="Textodecomentrio" /></w:pPr><w:r><w:rPr><w:rStyle w:val="Refdecomentrio" /></w:rPr><w:annotationRef /></w:r><w:r><w:t>Confirmar.</w:t></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r></w:p></w:comment><w:comment w:initials="BO" w:author="Breno Tiago Novello Trotta de Oliveira" w:date="2022-04-27T11:13:00Z" w:id="11"><w:p w:rsidR="00376655" w:rsidP="00376655" w:rsidRDefault="00376655" w14:paraId="4D302FFD" w14:textId="00697FA4"><w:r><w:t>Poderia colocar "Ciencia de Dados". Eu entendi o contexto, até acho que "aprendizado de maquina" faz mais sentido, mas usaria para enfatizar o nome do curso.</w:t></w:r><w:r><w:annotationRef /></w:r></w:p></w:comment><w:comment w:initials="ECG" w:author="Eduardo Correa Goncalves" w:date="2022-04-28T13:53:00Z" w:id="12"><w:p w:rsidR="00376655" w:rsidP="00376655" w:rsidRDefault="00376655" w14:paraId="4CADD5B3" w14:textId="77777777"><w:pPr><w:pStyle w:val="Textodecomentrio" /></w:pPr><w:r><w:rPr><w:rStyle w:val="Refdecomentrio" /></w:rPr><w:annotationRef /></w:r><w:r><w:t>Ok!</w:t></w:r></w:p></w:comment><w:comment w:initials="ECG" w:author="Eduardo Correa Goncalves" w:date="2022-04-28T14:00:00Z" w:id="13"><w:p w:rsidR="00376655" w:rsidP="00376655" w:rsidRDefault="00376655" w14:paraId="43EC173C" w14:textId="77777777"><w:pPr><w:pStyle w:val="Textodecomentrio" /></w:pPr><w:r><w:rPr><w:rStyle w:val="Refdecomentrio" /></w:rPr><w:annotationRef /></w:r></w:p></w:comment><w:comment w:initials="BO" w:author="Breno Tiago Novello Trotta de Oliveira" w:date="2022-04-27T11:34:00Z" w:id="14"><w:p w:rsidR="00D17359" w:rsidP="00D17359" w:rsidRDefault="00D17359" w14:paraId="182BE069" w14:textId="11641327"><w:r><w:t>Essa sigla de BASE DE DADOS  foi definida anteriormente no modulo 2?</w:t></w:r><w:r><w:annotationRef /></w:r></w:p></w:comment><w:comment w:initials="ECG" w:author="Eduardo Correa Goncalves" w:date="2022-04-28T14:12:00Z" w:id="15"><w:p w:rsidR="00D17359" w:rsidP="00D17359" w:rsidRDefault="00D17359" w14:paraId="664CCA73" w14:textId="77777777"><w:pPr><w:pStyle w:val="Textodecomentrio" /></w:pPr><w:r><w:rPr><w:rStyle w:val="Refdecomentrio" /></w:rPr><w:annotationRef /></w:r><w:r><w:t>Acho que não, troquei então no texto</w:t></w:r></w:p></w:comment><w:comment w:initials="MM" w:author="Marcelo Rodrigues de Holanda Maia" w:date="2022-08-23T16:56:35" w:id="763458662"><w:p w:rsidR="58EC5498" w:rsidRDefault="58EC5498" w14:paraId="22BED592" w14:textId="21C50234"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="58EC5498"><w:rPr /><w:t>Esta questão foi revisada no Módulo I.</w:t></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r></w:p><w:p w:rsidR="58EC5498" w:rsidRDefault="58EC5498" w14:paraId="08722176" w14:textId="3E03915F"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="58EC5498"><w:rPr /><w:t>Não é o problema que é supervisionado, mas sim o aprendizado aplicado ao problema.</w:t></w:r></w:p><w:p w:rsidR="58EC5498" w:rsidRDefault="58EC5498" w14:paraId="1009B77B" w14:textId="3544EA55"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="58EC5498"><w:rPr /><w:t>Sugestão de ajuste inserida no texto.</w:t></w:r></w:p></w:comment><w:comment w:initials="MM" w:author="Marcelo Rodrigues de Holanda Maia" w:date="2022-08-23T17:11:33" w:id="935760355"><w:p w:rsidR="58EC5498" w:rsidRDefault="58EC5498" w14:paraId="0C92C1E1" w14:textId="08468FA0"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="58EC5498"><w:rPr /><w:t>Sugeri a introdução do "em geral" porque essa afirmação não é válida no caso de classificação multi-rótulo.</w:t></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r></w:p></w:comment><w:comment w:initials="MM" w:author="Marcelo Rodrigues de Holanda Maia" w:date="2022-09-09T10:32:23" w:id="1733579351"><w:p w:rsidR="4A7A2789" w:rsidRDefault="4A7A2789" w14:paraId="1ECEC55B" w14:textId="58F9C426"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="4A7A2789"><w:rPr /><w:t>Nota para o designer: seria interessante usar recursos visuais para essa explicação. Podemos discutir possibilidades quando for implementar.</w:t></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r></w:p></w:comment><w:comment w:initials="MM" w:author="Marcelo Rodrigues de Holanda Maia" w:date="2022-09-09T10:47:34" w:id="179282308"><w:p w:rsidR="4A7A2789" w:rsidP="05D12FCE" w:rsidRDefault="4A7A2789" w14:paraId="751C900B" w14:textId="1A489A94"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="05D12FCE"><w:rPr /><w:t>A definição de partição "pura" apresentada anteriormente foi "partição em que há predominância de um rótulo de classe".</w:t></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r></w:p><w:p w:rsidR="4A7A2789" w:rsidP="05D12FCE" w:rsidRDefault="4A7A2789" w14:paraId="02B7CB9D" w14:textId="40BE5418"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="05D12FCE"><w:rPr /><w:t>Por essa definição, ambas as partições do exemplo seriam puras.</w:t></w:r></w:p><w:p w:rsidR="4A7A2789" w:rsidP="05D12FCE" w:rsidRDefault="4A7A2789" w14:paraId="6CD09EE0" w14:textId="76CCA158"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="05D12FCE"><w:rPr /><w:t>Sugiro rever um dos pontos para deixar consistente: ou o texto deste exemplo ou a definição de partições puras/impuras (talvez fosse mais preciso falar em impureza alta/baixa).</w:t></w:r></w:p></w:comment><w:comment w:initials="MM" w:author="Marcelo Rodrigues de Holanda Maia" w:date="2022-09-09T10:54:28" w:id="1313546104"><w:p w:rsidR="4A7A2789" w:rsidRDefault="4A7A2789" w14:paraId="0B879651" w14:textId="4238B748"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="4A7A2789"><w:rPr /><w:t>Esta definição de "partições puras" está inconsistente com o exemplo apresentado em seguida (ver comentário adicional no exemplo)</w:t></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r></w:p></w:comment><w:comment w:initials="MM" w:author="Marcelo Rodrigues de Holanda Maia" w:date="2022-09-09T11:56:44" w:id="462953646"><w:p w:rsidR="7AF3765A" w:rsidRDefault="7AF3765A" w14:paraId="6A0C7CFD" w14:textId="0136A6C9"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="7AF3765A"><w:rPr /><w:t>Não necessariamente, certo?</w:t></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r></w:p></w:comment><w:comment w:initials="MM" w:author="Marcelo Rodrigues de Holanda Maia" w:date="2022-09-09T14:15:00" w:id="574294490"><w:p w:rsidR="7AF3765A" w:rsidRDefault="7AF3765A" w14:paraId="5AD1A531" w14:textId="32175F11"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="7AF3765A"><w:rPr /><w:t>Este título ficou fora de contexto</w:t></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r></w:p></w:comment><w:comment w:initials="MM" w:author="Marcelo Rodrigues de Holanda Maia" w:date="2022-09-09T18:54:29" w:id="458632852"><w:p w:rsidR="7C0D74B8" w:rsidRDefault="7C0D74B8" w14:paraId="61D8663E" w14:textId="6386EBCC"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r w:rsidR="7C0D74B8"><w:rPr /><w:t>Alterei esse exemplo para contemplar a área de Geociências.</w:t></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r><w:r><w:rPr><w:rStyle w:val="CommentReference" /></w:rPr><w:annotationRef /></w:r></w:p></w:comment><w:comment w:initials="EG" w:author="Eduardo Correa Goncalves" w:date="2022-09-12T16:27:02" w:id="786161928"><w:p w:rsidR="73F6F145" w:rsidRDefault="73F6F145" w14:paraId="434C8C90" w14:textId="2CB7B98C"><w:pPr><w:pStyle w:val="CommentText" /></w:pPr><w:r><w:fldChar w:fldCharType="begin" /></w:r><w:r><w:instrText xml:space="preserve"> HYPERLINK "mailto:marcelo.h.maia@ibge.gov.br"</w:instrText></w:r><w:bookmarkStart w:name="_@_434E183ECABE4BC990AEEEA6AB0C99D7Z" w:id="498609156" /><w:r><w:fldChar w:fldCharType="separate" /></w:r><w:bookmarkEnd w:id=